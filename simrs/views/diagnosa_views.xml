<odoo>
    <record id="view_diagnosa_tree" model="ir.ui.view">
        <field name="name">simrs.diagnosa.tree</field>
        <field name="model">simrs.diagnosa</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="kode"/>
            </tree>
        </field>
    </record>

    <record id="view_diagnosa_form" model="ir.ui.view">
        <field name="name">simrs.diagnosa.form</field>
        <field name="model">simrs.diagnosa</field>
        <field name="arch" type="xml">
            <form string="Diagnosa ICD-10">
                <group>
                    <field name="name"/>
                    <field name="kode"/>
                </group>
            </form>
        </field>
    </record>

    <record id="action_diagnosa" model="ir.actions.act_window">
        <field name="name">Diagnosa icd10</field>
        <field name="res_model">simrs.diagnosa</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>
