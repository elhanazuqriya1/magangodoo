<odoo>
    <record id="view_pendaftaran_tree" model="ir.ui.view">
        <field name="name">simrs.pendaftaran.tree</field>
        <field name="model">simrs.pendaftaran</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="no_rm"/>
                <field name="nomor_antrian"/>
                <field name="pasien_id"/>
                <field name="unit_id"/>
                <field name="dokter_id"/>
                <field name="tanggal"/>
            </tree>
        </field>
    </record>

    <record id="view_pendaftaran_form" model="ir.ui.view">
        <field name="name">simrs.pendaftaran.form</field>
        <field name="model">simrs.pendaftaran</field>
        <field name="arch" type="xml">
            <form string="Pendaftaran">
                <group>
                    <field name="nomor_antrian" readonly="1"/>
                    <field name="pasien_id"/>
                    <field name="no_rm" readonly="1"/>
                    <field name="unit_id"/>
                    <field name="dokter_id"/>
                    <field name="tanggal"/>
                </group>
            </form>
        </field>
    </record>

    <record id="action_pendaftaran" model="ir.actions.act_window">
        <field name="name">Pendaftaran</field>
        <field name="res_model">simrs.pendaftaran</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>
